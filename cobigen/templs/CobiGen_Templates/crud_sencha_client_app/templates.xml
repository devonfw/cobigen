  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1">

  <templates>
    <templateExtension ref="${variables.etoName#cap_first}Controller.js"/>
    <templateExtension ref="MainController.js"/>
    <templateExtension ref="${variables.etoName#cap_first}.js" mergeStrategy="senchamerge"/>
    <templateExtension ref="${variables.etoName#cap_first}CrudVM.js" mergeStrategy="senchamerge"/>
    <templateExtension ref="${variables.etoName#cap_first}CrudVC.js"/>
    <templateExtension ref="${variables.etoName#cap_first}Crud.js" mergeStrategy="senchamerge"/>
    <templateExtension ref="${variables.etoName#cap_first}ListVM.js"/>
    <templateExtension ref="${variables.etoName#cap_first}ListVC.js"/>
