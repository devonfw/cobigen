<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<templatesConfiguration xmlns="http://capgemini.com/devonfw/cobigen/TemplatesConfiguration" version="2.1">
    <templates>
        <template name="valid" destinationPath="valid.txt" templateFile="valid.txt.ftl"/>
        <template name="invalid" destinationPath="invalid.txt" templateFile="invalid.txt.ftl"/>
    </templates>
    <increments>
        <increment name="i1" description="*">
            <templateRef ref="valid"/>
            <templateRef ref="invalid"/>
        </increment>
    </increments>
</templatesConfiguration>
