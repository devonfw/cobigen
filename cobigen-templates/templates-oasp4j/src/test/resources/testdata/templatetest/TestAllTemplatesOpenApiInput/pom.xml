<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.capgemini</groupId>
  <artifactId>Oasp4JTemplateTest-TestAllTemplatesOpenAPIInput</artifactId>
  <version>dev-SNAPSHOT</version>

  <build>
    <plugins>
      <plugin>
        <groupId>com.capgemini</groupId>
        <artifactId>cobigen-maven-plugin</artifactId>
        <version>3.2.0-SNAPSHOT</version>
        <executions>
          <execution>
            <id>generate</id>
            <phase>package</phase>
            <goals>
              <goal>generate</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <configurationFolder>${templatesProject}</configurationFolder>
          <inputFiles>
            <inputFile>OpenAPI.yaml</inputFile>
          </inputFiles>
          <templates>
            <template>ALL</template>
          </templates>
          <failOnNothingGenerated>true</failOnNothingGenerated>
        </configuration>
        <dependencies>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-tempeng-freemarker</artifactId>
            <version>1.0.0</version>
          </dependency>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-javaplugin</artifactId>
            <version>1.7.0-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-htmlplugin</artifactId>
            <version>1.1.0</version>
          </dependency>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-tsplugin</artifactId>
            <version>1.1.1</version>
          </dependency>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-jsonplugin</artifactId>
            <version>1.2.0</version>
          </dependency>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-xmlplugin</artifactId>
            <version>3.1.0</version>
          </dependency>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-openapiplugin</artifactId>
            <version>1.1.1-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.capgemini</groupId>
            <artifactId>cobigen-textmerger</artifactId>
            <version>1.2.0</version>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
  </build>

</project>
