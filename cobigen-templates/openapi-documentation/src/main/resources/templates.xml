<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<templatesConfiguration xmlns="http://capgemini.com/devonfw/cobigen/TemplatesConfiguration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5.0">

  <templates>
    <templateExtension ref="Documentation.adoc" mergeStrategy="textmerge_appendWithNewLine" />
  </templates>

  <templateScans>
    <templateScan templatePath="templates" destinationPath="" />
  </templateScans>

  <increments>
    <increment name="OpenAPI_Docs" description="Documentation" explanation="Contains information about the operations of the OpenAPI application.">
      <templateRef ref="Documentation.adoc" />
    </increment>
  </increments>
</templatesConfiguration>